<div class="container">
  <div class="full_background menu_background"></div>
  <div class="title_container">
    <div class="menu_title">Create New Trip</div>
  </div>
  <div class="new_trip_form">
    <form class="form-horizontal" role="form">
      <div class="form-group">
        <label class="col-sm-2 control-label">Trip Title:</label>
        <div class="col-sm-10">
          <input class="form-control" placeholder="Christmas in Maui">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label">Start Date:</label>
        <div class="col-sm-10">
          <input class="form-control" placeholder="12/22/2014">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label">End Date:</label>
        <div class="col-sm-10">
          <input class="form-control" placeholder="12/28/2014">
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <button type="submit" class="btn btn-default">Save Trip</button>
        </div>
      </div>
    </form>
  </div>
</div>
